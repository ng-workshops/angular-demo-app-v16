{
  "name": "angular-demo-app",
  "version": "1.0.0",
  "description": "Angular Demo Application to show some cool stuff :-)",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:all": "ng serve --proxy-config config/proxy.conf.json --configuration hmr & npm run start:server",
    "start:server": "node server",
    "start:client": "ng serve --proxy-config config/proxy.conf.json --configuration hmr",
    "start:dist": "ng build && DIST=true node server",
    "build": "ng build",
    "build:report": "ng build --stats-json && webpack-bundle-analyzer dist/angular-demo-app/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:cover": "jest --silent --coverage --no-cache && open-cli coverage/lcov-report/index.html",
    "jest": "jest --silent",
    "tdd": "jest --bail --expand --verbose",
    "tdd:cover": "jest --coverage --no-cache",
    "lint": "ng lint",
    "check": "ncu",
    "ci": "npm run lint && npm run jest && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.2",
    "@angular/cdk": "^16.2.1",
    "@angular/common": "^16.2.2",
    "@angular/compiler": "^16.2.2",
    "@angular/core": "^16.2.2",
    "@angular/forms": "^16.2.2",
    "@angular/material": "^16.2.1",
    "@angular/platform-browser": "^16.2.2",
    "@angular/platform-browser-dynamic": "^16.2.2",
    "@angular/router": "^16.2.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "16.1.1",
    "@angular-eslint/eslint-plugin": "16.1.1",
    "@angular-eslint/eslint-plugin-template": "16.1.1",
    "@angular-eslint/schematics": "16.1.1",
    "@angular-eslint/template-parser": "16.1.1",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.2",
    "@types/jasmine": "~4.3.5",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "eslint": "^8.48.0",
    "express": "^4.18.2",
    "jasmine-core": "~5.1.1",
    "jest": "^29.6.4",
    "jest-preset-angular": "^13.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-check-updates": "^16.13.1",
    "open-cli": "^7.2.0",
    "typescript": "~5.0.4",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}
